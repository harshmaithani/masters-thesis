  <build_depend>mtools_maths</build_depend>
  
  <run_depend>pluginlib</run_depend>
  <run_depend>actionlib</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>message_generation</run_depend>
  <run_depend>controller_interface</run_depend>
  <run_depend>realtime_tools</run_depend>
  <run_depend>kuka_lwr4p_hardware_interface</run_depend>
  <run_depend>kuka_lwr4p_controllers_msgs</run_depend>
  <run_depend>mtools_trajectory</run_depend>
  <run_depend>mtools_data_backup</run_depend>
  <run_depend>mtools_maths</run_depend>

  <export>
      <controller_interface plugin="${prefix}/kuka_lwr4p_controllers_plugins.xml"/>
  </export>
</package>
