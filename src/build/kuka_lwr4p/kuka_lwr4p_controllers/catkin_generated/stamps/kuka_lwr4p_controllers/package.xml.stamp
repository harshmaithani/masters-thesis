  <license>Distrubution under autorisation from IRT Jules Verne</license>

  <url type="repository">https://gitlab.irt-jules-verne.fr/svandernotte/kuka_lwr4p</url>

  <author email="sylvain.vandernotte@irt-jules-verne.fr">Sylvain Vandernotte</author>

  <buildtool_depend>catkin</buildtool_depend>
  
  <build_depend>pluginlib</build_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>std_msgs</build_depend>  
  <build_depend>message_generation</build_depend>  
  <build_depend>controller_interface</build_depend>
  <build_depend>realtime_tools</build_depend>  
  <build_depend>kuka_lwr4p_hardware_interface</build_depend>
  <build_depend>kuka_lwr4p_controllers_msgs</build_depend>
  <build_depend>mtools_trajectory</build_depend>
